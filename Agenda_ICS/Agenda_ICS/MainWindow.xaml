<Window x:Class="Agenda_ICS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Agenda_ICS"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <ScrollViewer Name="ScrollCalendar" DockPanel.Dock="Bottom" Height="30" />
            <DockPanel Name="Calendar" Background="LightCyan" SizeChanged="Window_SizeChanged" />
        </DockPanel>
    </Grid>
</Window>
